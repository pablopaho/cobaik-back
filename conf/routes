# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Bikes
GET     /bikes                      co.com.cobaik.bikes.BikesController.bikes
POST    /insert/bike                co.com.cobaik.bikes.BikesController.insertBike
POST    /available/bikes            co.com.cobaik.bikes.BikesController.availableBikes
GET     /bikes/$id<[0-9]+>          co.com.cobaik.bikes.BikesController.getBikeDetail(id: Int)



# Contact
POST   /contact                    co.com.cobaik.communications.contact.ContactController.sendContact
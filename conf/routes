# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Bikes
GET     /bikes                      co.com.akku.bikes.BikesController.bikes
POST    /insert/bike                co.com.akku.bikes.BikesController.insertBike

#Silhouette
OPTIONS /*path                                                                  co.com.akku.controllers.ApplicationController.options(path)

GET     /api/v1/authenticate/:provider                                          co.com.akku.users.silhouette.SocialAuthController.authenticate(provider)
POST    /api/v1/authenticate/:provider                                          co.com.akku.users.silhouette.SocialAuthController.authenticate(provider)


GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
# An example controller showing a sample home page
GET     /                           controllers.HomeController.index
# An example controller showing how to use dependency injection
#Add a comment to this line
GET     /count                      controllers.CountController.count
# An example controller showing how to write asynchronous code
GET     /message                    controllers.AsyncController.message